{
  "name": "barrel-defgen",
  "version": "1.0.1",
  "description": "Generates type definitions into a single file from a barrel file",
  "main": "./app/lib.js",
  "bin": {
    "barrel-defgen": "./bin/barrel-defgen"
  },
  "license": "MIT",
  "author": {
    "name": "David Recuenco",
    "url": "https://github.com/RecuencoJones"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RecuencoJones/barrel-defgen"
  },
  "bugs": {
    "url": "https://github.com/RecuencoJones/barrel-defgen/issues"
  },
  "engines": {
